{
	"name": "clvr7/curl-service",
	"description": "A composer plugin that provides simple helpers for calling both SOAP & REST API's in PHP.",
	"type": "library",
	"license": "MIT",
	"authors": [
		{
			"name": "clvr7"
		}
	],
	"require": {
		"php": ">=7.2"
	},
	"autoload": {
		"psr-4": {
			"Clvr7\\cURLService\\": "src/"
		}
	},
	"extra": {
		"laravel": {
			"providers": [
				"Clvr7\\cURLService\\cURLServiceProvider"
			]
		}
	},
	"minimum-stability": "dev"
}
