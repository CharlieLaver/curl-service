{
	"name": "charlielaver/curl-service",
	"description": "A composer plugin that provides simple helpers for calling both SOAP & REST API's in PHP.",
	"type": "library",
	"license": "MIT",
	"authors": [
		{
			"name": "Charlie Laver",
			"email": "charlielaver7@gmail.com"
		}
	],
	"require": {
		"php": ">=7.2"
	},
	"autoload": {
		"psr-4": {
			"CharlieLaver\\curlService\\": "src/"
		}
	},
	"extra": {
		"laravel": {
			"providers": [
				"CharlieLaver\\curlService\\curlServiceProvider"
			]
		}
	},
	"minimum-stability": "dev"
}
